{
  "version": 2,
  "monorepo": true,
  "github": {
    "enabled": true,
    "autoAlias": false
  },
  "projects": [
    {
      "name": "solanouns-webapp",
      "source": "packages/solanouns-webapp",
      "framework": "vite",
  "buildCommand": "cd ../.. && pnpm install && pnpm run build --workspace=@solanouns/webapp",
  "outputDirectory": "packages/solanouns-webapp/dist",
  "installCommand": "pnpm install",
      "rootDirectory": "packages/solanouns-webapp",
      "env": {
        "VITE_API_URL": "https://solanouns-api.vercel.app"
      }
    },
    {
      "name": "solanouns-api",
      "source": "packages/solanouns-api",
  "buildCommand": "cd ../.. && pnpm install",
  "installCommand": "pnpm install",
      "rootDirectory": "packages/solanouns-api"
    }
  ],
  "build": {
    "env": {
      "NPM_CONFIG_USER_AGENT": "pnpm"
    }
  }
}