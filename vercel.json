{
  "version": 2,
  "name": "solanouns-api",
  "builds": [
    {
      "src": "packages/solanouns-api/src/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["packages/solanouns-api/src/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "packages/solanouns-api/src/server.ts"
    }
  ],
  "functions": {
    "packages/solanouns-api/src/server.ts": {
      "includeFiles": "packages/solanouns-api/src/**"
    }
  }
}