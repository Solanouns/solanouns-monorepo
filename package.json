{
  "name": "solanouns-monorepo",
  "version": "1.0.0",
  "description": "The Solanouns DAO monorepo - A Solana fork of NounsDAO",
  "homepage": "https://solanouns.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Solanouns/solanouns-monorepo.git"
  },
  "license": "GPL-3.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
